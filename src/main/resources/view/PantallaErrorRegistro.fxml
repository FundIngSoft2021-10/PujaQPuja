<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.text.Font?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.image.Image?>

<BorderPane prefHeight="236.0" prefWidth="305.0" style="-fx-background-color: #FFFFFF;" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="pujaQpuja.controller.pantallas.PantallaErrorRegistroController">
    <center>
        <AnchorPane prefHeight="260.0" prefWidth="300.0" BorderPane.alignment="CENTER">
            <TextArea editable="false" layoutX="26.0" layoutY="25.0" prefHeight="134.0" prefWidth="252.0" style="-fx-font-size: 18;" text="Hubo un error en registrar usuario por campos erroneos&#10;" wrapText="true">
                <font>
                    <Font size="14.0"/>
                </font>
            </TextArea>
            <Button fx:id="botonRegresar" layoutX="22.0" layoutY="178.0" lineSpacing="5.0" mnemonicParsing="false" onAction="#accionRegresar" prefHeight="36.0" prefWidth="120.0" style="-fx-background-color: #74C3FF; -fx-background-radius: 8;"
                    text="Regresar" textAlignment="CENTER">
                <font>
                    <Font name="SansSerif Regular" size="20.0"/>
                </font>
                <opaqueInsets>
                    <Insets/>
                </opaqueInsets>
            </Button>
            <Button fx:id="botonCancelar" layoutX="162.0" layoutY="178.0" lineSpacing="5.0" mnemonicParsing="false" onAction="#accionCancelar" prefHeight="36.0" prefWidth="120.0" style="-fx-background-color: #74C3FF; -fx-background-radius: 8;"
                    text="Cancelar" textAlignment="CENTER">
                <font>
                    <Font name="SansSerif Regular" size="20.0"/>
                </font>
                <opaqueInsets>
                    <Insets/>
                </opaqueInsets>
            </Button>
            <ImageView fitHeight="57.0" fitWidth="67.0" layoutX="210.0" layoutY="92.0" pickOnBounds="true" preserveRatio="true">
                <Image url="@../images/alerta.png"/>
            </ImageView>
        </AnchorPane>
    </center>
</BorderPane>
